<launch>
  <node name="joystick" pkg="joy" type="joy_node">
    <param name="dev" value="/dev/input/js2"/>
  </node>

  <node name="controller2" pkg="robot03" type="controller2" output="screen">
	<param name="k_PWM" value="200.0"/>
	<param name="k_v" value="1.0"/>
	<param name="k_w" value="1.0"/>
	<param name="d" value="0.135"/>
  </node>

  <node pkg="rosserial_python" type="serial_node.py" name="arduino">
	<param name="port" value="/dev/ttyACM0"/>
	<param name="baud" value="57600"/>
  </node>

</launch>
